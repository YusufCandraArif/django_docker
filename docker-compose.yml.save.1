  version: '3'
    
  services:
    db:
      image: postgres
      environment:
        - POSTGRES_DB=erasysdb
        - POSTGRES_USER=postgres
        - POSTGRES_PASSWORD=pinpin
      expose:
       - "5432"
    web:
      build: .
      command: python manage.py runserver 0.0.0.0:8080
      volumes:
        - .:/code
      ports:
        - "8080:8080"
      depends_on:
        - db
